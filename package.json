{
  "name": "feelnnote-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:app": "turbo build --filter=app --filter=app-api",
    "build:bo": "turbo build --filter=bo --filter=bo-api",
    "dev": "turbo dev",
    "dev:app": "turbo dev --filter=app --filter=app-api",
    "dev:bo": "turbo dev --filter=bo --filter=bo-api",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "turbo": "^2.3.3",
    "prettier": "^3.4.2"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "npm@10.8.2"
}
